            <Button fx:id="Startmusic" layoutX="431.0" layoutY="585.0" mnemonicParsing="false" onMouseClicked="#Start" prefHeight="26.0" prefWidth="56.0" text="Play">
               <font>
                  <Font name="Times New Roman Bold Italic" size="13.0" />
