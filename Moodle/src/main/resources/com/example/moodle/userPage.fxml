         </children>
      </AnchorPane>
      <AnchorPane fx:id="listPane" layoutX="236.0" prefHeight="612.0" prefWidth="614.0">
         <children>
            <Button layoutX="543.0" layoutY="585.0" mnemonicParsing="false" onMouseClicked="#Reset" prefHeight="26.0" prefWidth="56.0" text="Reset">
               <font>
                  <Font name="Times New Roman Bold Italic" size="13.0" />
               </font>
            </Button>
            <TextArea fx:id="loginActivity" editable="false" layoutX="30.0" layoutY="528.0" prefHeight="58.0" prefWidth="237.0" />
